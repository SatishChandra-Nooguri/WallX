                            Padding="10" TextAlignment="Left" TextWrapping="NoWrap" Height="78" Width="825"/>
            </VisualBrush.Visual>
        </VisualBrush>

        <VisualBrush x:Key="search_contact_style" Stretch="None" AlignmentX="Left" AlignmentY="Top" >
